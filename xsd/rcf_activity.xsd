		    <xs:complexType>
			      <xs:complexContent>
				        <xs:extension base="rcfMarkedInteractionWithExample">
					          <xs:sequence maxOccurs="unbounded">
						            <xs:element name="prefix" type="xs:string" minOccurs="0"/>
						            <xs:element name="item" minOccurs="1" maxOccurs="unbounded">
							              <xs:complexType>
