									                  </xs:complexType>
								                </xs:element>
							              </xs:sequence>
						            </xs:complexType>
						            <xs:unique name="acceptableTypeInItemDUnique">
							              <xs:selector xpath=".//item"/>
							              <xs:field xpath="@id"/>
						            </xs:unique>
					          </xs:element>
					          <xs:element name="suffix" type="xs:string" minOccurs="0"/>
					          <xs:element name="hint" type="xs:string" minOccurs="0"/>
