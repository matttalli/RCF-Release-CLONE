							              </xs:complexType>
						            </xs:element>
						            <xs:element name="suffix" type="xs:string" minOccurs="0"/>
					          </xs:sequence>
					          <xs:attribute name="capitalise" type="capitaliseType" use="optional"/>
				        </xs:extension>
			      </xs:complexContent>
		    </xs:complexType>
	  </xs:element>
